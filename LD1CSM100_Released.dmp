Object ObjectList
Numeric DisplayTimeOut, LastKeyStroke, SetPointFilter
String 90 Str_ObjectPath
String MenuString
Datetime Line3Temp

Line Init
  DisplayTimeOut = 100
  Str_ObjectPath = LD1CtrlName
  DisplayMode = 0
  AnswerString = ""
  If LD1UserType = "Op" or LD1UserType = "AccAdmin" then
    MenuString = "     EDIT   NEXT"
  Else
    MenuString = "            NEXT"
  Endif


  If LD1InfCtrlName <> "" then
    Str_ObjectPath = Str_ObjectPath ; " " ; LD1InfCtrlName
  Endif
  If LD1PointType = "SetPoints" then
    LD1PointType = "Numeric"
    SetPointFilter = On
  Else
    SetPointFilter = Off
  Endif
  Goto CtrlOpenList

Line CtrlOpenList
  If OpenList(LD1PointType, ObjectList, getname(Str_ObjectPath)) = Success then
    Goto GetObjectName
  Else
    Goto CloseObjectList
  Endif

Line GetObjectName
  If GetObject(ObjectList) = Success then
    If SetPointFilter = On then
      If ObjectList Setpoint = False then
        Goto GetObjectName
      Endif
    Endif
    DisplayLine1 = ObjectList Name
    DisplayLine2 = "The value is:-"
    DisplayLine3 = ""
    Print ObjectList Value to DisplayLine3
    DisplayLine4 = MenuString
    Goto waitkeypress
  Else
    Goto CloseObjectList
  Endif

Line waitkeypress
  If KeyStroke > 0 then Goto TestKeyPress
  If AnswerString <> "" then Goto UpdateEdit
  If TS > DisplayTimeOut then
    Goto CloseObjectList
    Start LD1_Init
    Stop 
  Endif

Line TestKeyPress
  LastKeyStroke = KeyStroke
  KeyStroke = 0
  Goto F3
Line F3
  If LastKeyStroke = 19 then 'F3 - next
    If GetObject(ObjectList) = Success then
      If SetPointFilter = On then
        If ObjectList Setpoint = False then
          Goto F3
        Endif
      Endif
      DisplayLine1 = ObjectList Name

'use USA workaround here
      If LD1PointType = "DateTime" then
        Line3Temp = ObjectList Value
'USA workaround code

        DisplayLine3 = timepiece(Month, Line3Temp) ; "/" ; timepiece(Dayofmonth, Line3Temp) ; "/" ; right(timepiece(Year, Line3Temp), 2) ; " "

        If timepiece(Hour, Line3Temp) >= 12 then

          If timepiece(Hour, Line3Temp) = 12 then
            DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3Temp) ; ":" ; timepiece(Minute, Line3Temp) ; "PM"
          Else
            DisplayLine3 = DisplayLine3 ; (timepiece(Hour, Line3Temp) - 12) ; ":" ; timepiece(Minute, Line3Temp) ; "PM"
          Endif

        Else

          If timepiece(Hour, Line3Temp) = 0 then
            DisplayLine3 = DisplayLine3 ; "12:" ; timepiece(Minute, Line3Temp) ; "AM"
          Else
            DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3Temp) ; ":" ; timepiece(Minute, Line3Temp) ; "AM"
          Endif

        Endif
'end USA workaround code

      Else

        Print ObjectList Value to DisplayLine3
      Endif

      Goto waitkeypress
    Else
      CloseList(ObjectList)

      If OpenList(LD1PointType, ObjectList, getname(Str_ObjectPath)) = Success then
        If GetObject(ObjectList) = Success then
          If SetPointFilter = On then
            If ObjectList Setpoint = False then
              Goto F3
            Endif
          Endif
          DisplayLine1 = ObjectList Name
          Print ObjectList Value to DisplayLine3
        Else
          Goto CloseObjectList
        Endif
      Else
        Goto CloseObjectList
      Endif
    Endif
  Endif

  Goto F2

Line F2
  If LastKeyStroke = 18 then 'F2
    AnswerString = ""
    If LD1UserType = "AccAdmin" then
      If LD1PointType = "Numeric" then DisplayMode = 1
      If LD1PointType = "String" then DisplayMode = 2
      If LD1PointType = "DateTime" then DisplayMode = 3
'**   If LD1PointType = "SetPoints" then DisplayMode = 1
    Else
      DisplayMode = 0
    Endif
  Endif
