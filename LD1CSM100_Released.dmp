  Object : LD1_Scheduler
   Type : InfinityProgram
   LastChange : 06/12/03 02:22:45 PM
   DeviceId : LabLD1\Infinity1
   Alias : LD1_Scheduler
   CreateTime : 01/29/03 02:33:24 PM
   CreatedBy : Root\Acc
   TimeLocked : 06/13/03 03:04:53 PM
   AutoStart : False
   ByteCode : 
'Name: LD1_SelectSched
'FlowType: Looping
'AutoStart: False
'CommandLine: False
'References

'EndReferences
Numeric DisplayTimeOut, LastKeyStroke, SchedPos, GotSchedOn, GotSchedOff
Numeric DispHour, DispMin, DispDay
String MenuString, DayList[7]
Datetime temp
Datetime Line3temp

Line Init
  DayList[1] = "Sun"
  DayList[2] = "Mon"
  DayList[3] = "Tue"
  DayList[4] = "Wed"
  DayList[5] = "Thu"
  DayList[6] = "Fri"
  DayList[7] = "Sat"
  SchedPos = 1
  GotSchedOn = False
  GotSchedOff = False
  DisplayTimeOut = 60
  DisplayMode = 0
  RedLED = Off
  GreenLED = On
  DisplayLine1 = "Select Schedule"
  DisplayLine2 = "Enter Schedule"
  DisplayLine3 = "Number 1 to 8"
  DisplayLine4 = ""
  AnswerString = ""
  If LD1UserType = "Op" or LD1UserType = "AccAdmin" then
    MenuString = "     EDIT   NEXT"
  Else
    MenuString = "            NEXT"
  Endif

  Goto waitkey

Line waitkey
  LastKeyStroke = KeyStroke
  KeyStroke = 0
  If AnswerString <> "" and DisplayMode = 3 then Goto UpdateSched
  If TS > DisplayTimeOut then
    Start LD1_MenuSelect
    Stop 
  Endif
  Basedon LastKeyStroke Goto K1, K2, K3, K4, K5, K6, K7, K8, K9, K0, K11, K12, Kup, Kdn, Kcc, Ken, Kf1, Kf2, Kf3
Line K1
  LD1_SchedSel = 1
  Goto ViewSched
Line K2
  LD1_SchedSel = 2
  Goto ViewSched
Line K3
  LD1_SchedSel = 3
  Goto ViewSched
Line K4
  LD1_SchedSel = 4
  Goto ViewSched
Line K5
  LD1_SchedSel = 5
  Goto ViewSched
Line K6
  LD1_SchedSel = 6
  Goto ViewSched
Line K7
  LD1_SchedSel = 7
  Goto ViewSched
Line K8
  LD1_SchedSel = 8
  Goto ViewSched
Line K9
  Goto waitkey
Line K0
  Goto waitkey
Line K11
  Goto waitkey
Line K12
  Goto waitkey
Line Kup
  Goto waitkey
Line Kdn
  Goto waitkey
Line Kcc
  Goto closeobjectlist
Line Ken
  Goto waitkey
Line Kf1
  Goto waitkey
Line Kf2

  If LD1UserType = "Op" or LD1UserType = "AccAdmin" then
    If GotSchedOn = True then
' Print GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos) to DisplayLine3
      Line3temp = GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos)

'USA workaround code

      DisplayLine3 = timepiece(Month, Line3temp) ; "/" ; timepiece(Dayofmonth, Line3temp) ; "/" ; right(timepiece(Year, Line3temp), 2) ; " "

      If timepiece(Hour, Line3temp) >= 12 then

        If timepiece(Hour, Line3temp) = 12 then
          DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3temp) ; ":" ; timepiece(Minute, Line3temp) ; "PM"
        Else
          DisplayLine3 = DisplayLine3 ; (timepiece(Hour, Line3temp) - 12) ; ":" ; timepiece(Minute, Line3temp) ; "PM"
        Endif

      Else

        If timepiece(Hour, Line3temp) = 0 then
          DisplayLine3 = DisplayLine3 ; "12:" ; timepiece(Minute, Line3temp) ; "AM"
        Else
          DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3temp) ; ":" ; timepiece(Minute, Line3temp) ; "AM"
        Endif

      Endif
'end USA workaround code


    Endif
    If GotSchedOff = True then
' Print GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos) to DisplayLine3
      Line3temp = GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos)
'USA workaround code

      DisplayLine3 = timepiece(Month, Line3temp) ; "/" ; timepiece(Dayofmonth, Line3temp) ; "/" ; right(timepiece(Year, Line3temp), 2) ; " "

      If timepiece(Hour, Line3temp) >= 12 then

        If timepiece(Hour, Line3temp) = 12 then
          DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3temp) ; ":" ; timepiece(Minute, Line3temp) ; "PM"
        Else
          DisplayLine3 = DisplayLine3 ; (timepiece(Hour, Line3temp) - 12) ; ":" ; timepiece(Minute, Line3temp) ; "PM"
        Endif

      Else

        If timepiece(Hour, Line3temp) = 0 then
          DisplayLine3 = DisplayLine3 ; "12:" ; timepiece(Minute, Line3temp) ; "AM"
        Else
          DisplayLine3 = DisplayLine3 ; timepiece(Hour, Line3temp) ; ":" ; timepiece(Minute, Line3temp) ; "AM"
        Endif

      Endif
'end USA workaround code

    Endif


    DisplayMode = 3
'set cursor position
    CursorPosition = 10

  Endif
  Goto waitkey

Line Kf3
  If SchedPos = 15 then Goto closeobjectlist
  If SchedPos is between 1 and 14 then
    If GotSchedOn = True then
      GotSchedOn = False
      Goto SchedOFF
    Endif


    If GotSchedOff = True then
      GotSchedOff = False
      SchedPos = SchedPos + 1
      Goto SchedON
    Endif
  Endif

  Goto waitkey

Line ViewSched
  Goto SchedON


Line SchedON
  GotSchedOn = True
  If SchedPos is between 1 and 7 then

    DisplayLine1 = "Schedule " ; NumToStr(LD1_SchedSel)
    DisplayLine2 = DayList[SchedPos] ; " ON 1 is"
    DispHour = timepiece(Hour, GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos))
    DispMin = timepiece(Minute, GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos))
    If DispMin < 10 then
      Print NumToStr(DispHour) ; ":" ; "0" ; NumToStr(DispMin) to DisplayLine3
    Else
      Print NumToStr(DispHour) ; ":" ; NumToStr(DispMin) to DisplayLine3
    Endif
    DisplayLine4 = MenuString
  Endif

  If SchedPos is between 8 and 14 then

    DisplayLine1 = "Schedule " ; NumToStr(LD1_SchedSel)
    DisplayLine2 = DayList[SchedPos - 7] ; " ON 2 is"
    DispHour = timepiece(Hour, GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos))
    DispMin = timepiece(Minute, GetLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos))
    If DispMin < 10 then
      Print NumToStr(DispHour) ; ":" ; "0" ; NumToStr(DispMin) to DisplayLine3
    Else
      Print NumToStr(DispHour) ; ":" ; NumToStr(DispMin) to DisplayLine3
    Endif

    DisplayLine4 = MenuString
  Endif

  Goto waitkey

Line SchedOFF
  GotSchedOff = True
  If SchedPos is between 1 and 7 then
    DisplayLine1 = "Schedule " ; NumToStr(LD1_SchedSel)
    DisplayLine2 = DayList[SchedPos] ; " OFF 1 is"
    DispHour = timepiece(Hour, GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos))
    DispMin = timepiece(Minute, GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos))
    If DispMin < 10 then
      Print NumToStr(DispHour) ; ":" ; "0" ; NumToStr(DispMin) to DisplayLine3
    Else
      Print NumToStr(DispHour) ; ":" ; NumToStr(DispMin) to DisplayLine3
    Endif


    DisplayLine4 = MenuString
  Endif

  If SchedPos is between 8 and 14 then
    DisplayLine1 = "Schedule " ; NumToStr(LD1_SchedSel)
    DisplayLine2 = DayList[SchedPos - 7] ; " OFF 2 is"
    DispDay = timepiece(Weekday, GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos))
    DispHour = timepiece(Hour, GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos))
    DispMin = timepiece(Minute, GetLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos))

    If DispMin < 10 then
      Print NumToStr(DispHour) ; ":" ; "0" ; NumToStr(DispMin) to DisplayLine3
    Else
      Print NumToStr(DispHour) ; ":" ; NumToStr(DispMin) to DisplayLine3
    Endif

    DisplayLine4 = MenuString
  Endif

  Goto waitkey


Line UpdateSched
  If GotSchedOn = True then
    temp = StrToDate(AnswerString)
    PassLog(getname("SchedOnTime" ; LD1_SchedSel), SchedPos, temp)
  Endif
  If GotSchedOff = True then
    temp = StrToDate(AnswerString)
    PassLog(getname("SchedOffTime" ; LD1_SchedSel), SchedPos, temp)
  Endif



  AnswerString = ""
  CursorPosition = 1
  DisplayMode = 0

  Goto waitkey

Line closeobjectlist
  LD1_DispClear 
  Start LD1_MenuSelect
  Stop 


   EndByteCode
   DefaultPort : comm3
  EndObject

