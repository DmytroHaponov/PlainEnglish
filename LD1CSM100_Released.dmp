  Object : Optimum
   Type : InfinityFunction
   LastChange : 05/28/03 10:10:03 AM
   DeviceId : LabLD1\Infinity1
   Alias : Optimum
   CreateTime : 05/28/03 10:10:03 AM
   CreatedBy : Root\Acc
   TimeLocked : 06/12/03 11:09:28 AM
   ByteCode : 
'***  V6.0 - 1/10/99 ***
'Time = Date
Arg 1 StartSp
Arg 2 StopSp
Arg 3 SpaceTemp
Arg 4 OAT
Arg 5 StartTime
Arg 6 StopTime
Arg 7 MaxStart
Arg 8 MaxStop
Arg 9 LogTime
Arg 10 CalcTime
Arg 11 AllowedTime
Arg 12 SLA
Arg 13 WarmUp
Arg 14 Learn
Numeric Mode, OATPtr, DiffTemp, OATFac, NewRate, i, x, LastMode
Numeric MaxStop1, MaxStart1, WarmUp1, WarmUpReq
Datetime LastOn
If SLA Size <> 80 or StopTime = StartTime then Return (0)
MaxStop1 = (MaxStop + 1) * 60
MaxStart1 = (MaxStart) * 60
WarmUp1 = (WarmUp) * 60
LastMode and Mode = SLA
If LogTime[3] <> LogTime[7] then LastOn = LogTime[3]
If (Date - LastOn) > 86400 then WarmUpReq = On
If Mode <> 0 and (Date > LogTime[5]) then WarmUpReq = Off
If SLA[41] = 0 then
  x = 1.5
  For i = 1 to 31  
    SLA[41 - i] = x
    x = x - 0.04
  Next i
  x = 1.5
  For i = 1 to 31  
    SLA[49 + i] = x
    x = x - 0.04
    SLA[41] = 1
  Next i
Endif
If Mode = 0 then
  If Date is between StartTime and (StopTime - 60) and SLA[46] = 0 then
    Mode = 2
    LogTime = Date
  Endif
  DiffTemp = StartSp - SpaceTemp
  OATPtr = max(minimum(40 - (20 - OAT), 40), 10)
  If DiffTemp > 0 & ((StopTime - StartTime) > 300) then CalcTime = StartTime - (DiffTemp / max(SLA[OATPtr], 0.04) * 3600) Else CalcTime = StartTime
  If CalcTime is between (StartTime - MaxStart1 - (WarmUp1 * WarmUpReq)) and StartTime then
    AllowedTime = CalcTime
  Else
    If CalcTime > StartTime then AllowedTime = StartTime Else AllowedTime = StartTime - MaxStart1 - (WarmUp1 * WarmUpReq)
  Endif
  If Date is between (StartTime - MaxStart1 - (WarmUp1 * WarmUpReq)) and StartTime then
    If Date > CalcTime then
      Mode = 1
      LogTime and LogTime[1] = Date
      LogTime[2], LogTime[3] and LogTime[4] = LogTime[7]
      SLA[2] = OAT
      SLA[3] = SpaceTemp
      SLA[4] = DiffTemp
      SLA[5] = OATPtr
      SLA[9] = StartSp
      SLA[7], SLA[42], SLA[43] and SLA[47] = 0
    Endif
  Endif
  If Mode <> 0 then
    LogTime[5] = StartTime
    LogTime[6] = StopTime
  Endif
Endif
If Mode = 1 then
  OATPtr = SLA[5]
  If Date is not between (StartTime - MaxStart1 - (WarmUp1 * WarmUpReq)) and (StopTime - MaxStop1) then
    Mode = 0
    LogTime and LogTime[2] = Date
  Endif
  If Date is between (StopTime - MaxStop1) and (StopTime) then
    Mode = 2
    LogTime and LogTime[2] = Date
  Endif
  If Mode = 1 then
    If SpaceTemp >= StartSp then
      If Learn then
        If (Date - LogTime) = 0 then LogTime = Date - 1
        NewRate = minimum((SLA[4] / ((Date - LogTime) / 3600)), 2 * SLA[OATPtr])
        SLA[OATPtr] = max((SLA[OATPtr] * 0.6 + NewRate * 0.4), 0.05)
        If OATPtr > 10 then
          For i = OATPtr to 11  
            If SLA[i - 1] > NewRate then SLA[i - 1] = max((SLA[i - 1] * 0.6 + NewRate * 0.4), 0.05)
          Next i
          If OATPtr < 39 then
            For i = OATPtr to 38  
              NewRate = (i * ((SLA[40] - SLA[OATPtr]) / (40 - OATPtr))) + SLA[40] - 40 * (SLA[40] - SLA[OATPtr]) / (40 - OATPtr)
              If SLA[i + 1] > NewRate then SLA[i + 1] = max((SLA[i + 1] * 0.6 + NewRate * 0.4), 0.05)
            Next i
          Endif
        Endif
      Endif
      Mode = 2
      LogTime and LogTime[2] = Date
      SLA[7] = SpaceTemp
      SLA[9] = StartSp
    Endif
  Endif
Endif
If LastMode = 0 & Mode = 2 then
  LogTime[1] and LogTime[2] = Date
  LogTime[3] and LogTime[4] = LogTime[7]
  SLA[2] = OAT
  SLA[3] and SLA[7] = SpaceTemp
  SLA[9] = StartSp
  SLA[42], SLA[43] and SLA[47] = 0
Endif
If Mode = 2 then
  If Date is not between (StartTime - MaxStart1 - (WarmUp1 * WarmUpReq)) and (StopTime - 60) then
    Mode = 0
    LogTime = Date
  Endif
  DiffTemp = SpaceTemp - StopSp
  OATPtr = max(minimum(80 - (20 - OAT), 80), 50)
  If DiffTemp > 0 then CalcTime = StopTime - (DiffTemp / max(SLA[OATPtr], 0.04) * 3600) Else CalcTime = StopTime
  If CalcTime is between (StopTime - MaxStop1) and StopTime then
    AllowedTime = CalcTime
  Else
    If CalcTime > (StopTime - 60) then AllowedTime = StopTime Else AllowedTime = StopTime - MaxStop * 60
  Endif
  If Date is between (StopTime - MaxStop1) and StopTime then
    If Date > CalcTime then
      Mode = 3
      LogTime and LogTime[3] = Date
      LogTime[4] = LogTime[7]
      SLA[42] = OAT
      SLA[43] = SpaceTemp
      SLA[44] = DiffTemp
      SLA[45] = OATPtr
      SLA[49] = StopSp
      SLA[46] = 1
    Endif
  Endif
Endif
If Mode = 3 then
  x = 0
  OATPtr = SLA[45]
  If Date is not between (StopTime - MaxStop1) and StopTime then Mode = 0
  If SpaceTemp <= StopSp then x = 1
  If Date >= (StopTime - 60) then x = 2
  If x > 0 then
    If Learn & MaxStop <> 0 & ((Date - LogTime[3]) < MaxStop * 60) then
      If (Date - LogTime) = 0 then LogTime = Date - 1
      SLA[OATPtr] = minimum(max(((SLA[OATPtr] * 0.6 + (SLA[44] / ((Date - LogTime) / 3600)) * 0.4) * (x = 1) + SLA[OATPtr] * 0.9 * (x = 2)), 0.05), 2 * SLA[OATPtr])
      If OATPtr < 80 then
        For i = OATPtr to 79  
          If SLA[i + 1] > SLA[OATPtr] then SLA[i + 1] = max((SLA[i + 1] * 0.6 + SLA[OATPtr] * 0.4), 0.05)
        Next i
        If OATPtr > 51 then
          For i = OATPtr to 52  
            NewRate = (i * ((SLA[50] - SLA[OATPtr]) / (50 - OATPtr))) + SLA[50] - 50 * (SLA[50] - SLA[OATPtr]) / (50 - OATPtr)
            If SLA[i - 1] > NewRate then SLA[i - 1] = max((SLA[i - 1] * 0.6 + NewRate * 0.4), 0.05)
          Next i
        Endif
      Endif
    Endif
    Mode = 0
    LogTime and LogTime[4] = Date
    SLA[47] = SpaceTemp
    SLA[49] = StopSp
  Endif
Endif
If (LastMode = 3 or LastMode = 2) & Mode = 0 then SLA[48] = 1
If Date is not between StartTime and (StopTime - 60) then SLA[46] = 0
LastMode, SLA[1] and SLA = Mode
   EndByteCode
  EndObject

