  LD1_DispClear 
  AnswerString = ""


  Goto CtrlOpenList

Line CtrlOpenList
  If OpenList("Controller", NetCtrl) = Success then
    Goto GetCtrlrName
  Else
    CloseList(NetCtrl)
    Start LD1_Init
    Stop 
  Endif

Line GetCtrlrName
  If GetObject(NetCtrl) = Success then
    LD1CtrlName = NetCtrl Name
    DisplayMode = 0
    DisplayLine1 = "Ethernet"
    DisplayLine2 = "Controller Name"
    DisplayLine4 = "ACCEPT      NEXT"
    Goto WaitKeyPress

  Else
    CloseList(NetCtrl)
    Start LD1_Init
    Stop 
  Endif

Line WaitKeyPress
  If KeyStroke > 0 then Goto TestKeyPress
  If TS > DispTimeOut then
    CloseList(NetCtrl)
    Start LD1_Init
    Stop 
  Endif

Line TestKeyPress
  LastKeyStroke = KeyStroke
  KeyStroke = 0
'DisplayLine1 = "Controller Name"
  If LastKeyStroke = 19 then 'F3 next ctrlr
    DisplayLine1 = "Controller Name"
    If GetObject(NetCtrl) = Success then
      LD1CtrlName = NetCtrl Name
      DisplayLine2 = NetCtrl Name
      If LD1InfCtrlName <> "" then
        LD1InfCtrlName = ""
        CloseList(InfCtrl)
      Endif
      Goto TestKeyPress
    Else
      CloseList(NetCtrl)

      If OpenList("Controller", NetCtrl) = Success then
        If GetObject(NetCtrl) = Success then
          LD1CtrlName = NetCtrl Name
          DisplayLine2 = LD1CtrlName
        Else
          CloseList(NetCtrl)
          Start LD1_Init
          Stop 
        Endif
      Else
        CloseList(NetCtrl)
        Start LD1_Init
        Stop 
      Endif
    Endif
    Goto TestKeyPress
  Endif

  If LastKeyStroke = 14 then 'down arrow
    DisplayLine1 = "Infinet Device"
    If LD1InfCtrlName = "" then
      If OpenList("Infinetctlr", InfCtrl, NetCtrl) = Success then
        If GetObject(InfCtrl) = Success then
          LD1InfCtrlName = InfCtrl Name
        Else
          LD1InfCtrlName = ""
          CloseList(InfCtrl)
        Endif
      Else
        LD1InfCtrlName = ""
        CloseList(InfCtrl)
      Endif
    Else
      If GetObject(InfCtrl) = Success then
        LD1InfCtrlName = InfCtrl Name
      Else
        LD1InfCtrlName = ""
        CloseList(InfCtrl)
      Endif
    Endif
    DisplayLine2 = LD1InfCtrlName
    Goto TestKeyPress

  Endif

  If LastKeyStroke = 15 then ' cancel key
    DisplayLine1 = ""
    DisplayLine2 = ""
    CloseList(NetCtrl)
    If LD1InfCtrlName <> "" then
      CloseList(InfCtrl)
    Endif
    Start LD1_Password
    Stop 
    Goto TestKeyPress
  Endif
  If LastKeyStroke = 17 then ' F1 key
    If LD1InfCtrlName = "" then
      CloseList(NetCtrl)
    Endif

    Start LD1_SelectPtType
    Stop 
    Goto TestKeyPress
  Endif

  If TS > DispTimeOut then Goto WaitKeyPress









   EndByteCode
   DefaultPort : comm3
  EndObject

