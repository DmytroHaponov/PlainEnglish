  Object : CXSched
   Type : InfinityFunction
   LastChange : 06/12/03 11:23:34 AM
   DeviceId : LabLD1\Infinity1
   Alias : CXSched
   CreateTime : 01/22/03 04:19:54 PM
   CreatedBy : Root\Acc
   TimeLocked : 06/12/03 04:40:14 PM
   ByteCode : 
'V3.0
Arg 1 StartTime
Arg 2 StopTime
Arg 3 Sched
Arg 4 SchedNum
Numeric MaxArraySize, count, NextDay, NoOfShifts
Datetime DefaultTime.1, StopTimeStore, StartTimeStore
NextDay = Weekday Mod 7 + 1
MaxArraySize = minimum(StartTime Size, StopTime Size)
NoOfShifts = truncate(MaxArraySize / 7)

If ((StartTime[Weekday] - DefaultTime.1) > 86400) or ((StopTime[Weekday] - DefaultTime.1) > 86400) or ((StartTime[NextDay] - DefaultTime.1) > 86400) or ((StopTime[NextDay] - DefaultTime.1) > 86400) then
  For count = 1 to MaxArraySize  
    StartTime[count] = DefaultTime.1 + timepiece(Hour, StartTime[count]) * 3600 + timepiece(Minute, StartTime[count]) * 60 + 30
    StopTime[count] = DefaultTime.1 + timepiece(Hour, StopTime[count]) * 3600 + timepiece(Minute, StopTime[count]) * 60 + 30
  Next count
Endif

StopTimeStore = DefaultTime.1
For count = 1 to NoOfShifts  
  If Date >= StopTimeStore then
    StartTimeStore = Date - Hour * 3600 - Minute * 60 - Second + (StartTime[Weekday + (count - 1) * 7] - DefaultTime.1)
    StopTimeStore = Date - Hour * 3600 - Minute * 60 - Second + (StopTime[Weekday + (count - 1) * 7] - DefaultTime.1)
  Endif
  If timepiece(Minute, StopTimeStore) = 59 then StopTimeStore = StopTimeStore + 30

Next count

If Date >= StopTimeStore then
  StartTimeStore = Date - Hour * 3600 - Minute * 60 - Second + (StartTime[NextDay] - DefaultTime.1) + 86400
  StopTimeStore = Date - Hour * 3600 - Minute * 60 - Second + (StopTime[NextDay] - DefaultTime.1) + 86400
Endif

StopTime = StopTimeStore
StartTime = StartTimeStore

If passed(3) then
  If Date is between StartTime and StopTime then
    Sched = On
    If SchedNum = 1 then SchedOnTime1 = StartTime
    If SchedNum = 1 then SchedOffTime1 = StopTime
    If SchedNum = 2 then SchedOnTime2 = StartTime
    If SchedNum = 2 then SchedOffTime2 = StopTime
    If SchedNum = 3 then SchedOnTime3 = StartTime
    If SchedNum = 3 then SchedOffTime3 = StopTime
    If SchedNum = 4 then SchedOnTime4 = StartTime
    If SchedNum = 4 then SchedOffTime4 = StopTime
    If SchedNum = 5 then SchedOnTime5 = StartTime
    If SchedNum = 5 then SchedOffTime5 = StopTime
    If SchedNum = 6 then SchedOnTime6 = StartTime
    If SchedNum = 6 then SchedOffTime6 = StopTime
    If SchedNum = 7 then SchedOnTime7 = StartTime
    If SchedNum = 7 then SchedOffTime7 = StopTime
    If SchedNum = 8 then SchedOnTime8 = StartTime
    If SchedNum = 8 then SchedOffTime8 = StopTime

  Else
    Sched = Off
  Endif
Endif

   EndByteCode
   CommandLine : False
  EndObject

